{
	"id": "com.sap.sample.coloredbox.mobile",
	"version": "2.0.0",
	"name": "Colored Box (mobile support_noflag)",
	"description": "A colored box for testing mobile support",
	"newInstancePrefix": "ColoredBox",
	"icon": "https://37wh5r.github.io/custom-widget-samples/coloredboxmobile/icon.png",
	"vendor": "SAP",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-coloredbox-mobile",
			"url": "https://37wh5r.github.io/custom-widget-samples/coloredboxmobile/coloredbox_main.js",
			"integrity": "sha256-oqsfB2Q6HbuxKuI4sbNL6RQSHC4gHOxGmLwqiYIK6EA=",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-sap-sample-coloredbox-styling-mobile",
			"url": "https://37wh5r.github.io/custom-widget-samples/coloredboxmobile/coloredbox_styling.js",
			"integrity": "sha256-GTnm+Uc75SAGTTjBEr1QqtwXjXVlD6L4MMZbM0z2Va0=",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-sap-sample-coloredbox-builder-mobile",
			"url": "https://37wh5r.github.io/custom-widget-samples/coloredboxmobile/coloredbox_builder.js",
			"integrity": "sha256-+5erR8Ay33ebfRPYe9KTfl4hlfBgQoKaZY1lTaacaWQ=",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "red"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"showDimensions": {
			"type": "boolean",
			"description": "Show Dimensions",
			"default": false
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"parameters": [],
			"body": "return this.color;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Colored Box."
		}
	}
}
